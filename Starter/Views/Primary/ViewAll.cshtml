 @model Starter.DAO.StarterDbContext
 <h1>View All</h1>
    <table class="table">
        <thead>
            <tr>
            <th scope="col">ID</th>
            <th scope="col">Nickname</th>
            <th scope="col">Name</th>
            <th scope="col">Role</th>
            <th scope="col">Email</th> 
            <th scope="col">Phone</th> 
            </tr>
        </thead>
        <tbody>
        @foreach (ClubMemberModel clubMember in @Model.clubMembers)
        {
            <tr>
                <th scope="row">@clubMember.id</th>
                <td><a asp-area="" asp-controller="Primary" asp-action="ViewOne" asp-route-clubMemberID ="@clubMember.id">@clubMember.nickName</a> </td>
                <td>@clubMember.firstName @clubMember.lastName</td>
                <td>@clubMember.clubRole</td>
                <td>@clubMember.email</td>
                <td>@clubMember.phone</td>
            </tr>
        }            
        </tbody>
    </table>